// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model btcusdt_1m {
  id                       Int    @id @default(autoincrement())
  openTime                 String @default("0")
  open                     String @default("0")
  high                     String @default("0")
  low                      String @default("0")
  close                    String @default("0") //lastest price
  volume                   String @default("0")
  closeTime                String @default("0")
  baseAssetVolume          String @default("0")
  numberOfTrades           String @default("0")
  takerBuyQuoteAssetVolume String @default("0")
  takerBuyBaseAssetVolume  String @default("0")
  ignore                   String @default("0")

  @@unique([openTime])
  @@index([openTime], type: Hash)
}

